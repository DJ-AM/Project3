<!DOCTYPE html>
<html>
  <head>
    <title>Roadtrippr</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  <!-- Bootstrap -->
    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">

    <!-- jQuery library -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>

    <!-- Latest compiled JavaScript -->
    <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
  </head>

  <body>

  <div class="jumbotron">
    <img src="../../assets/images/roadtrippr.png" class="img-responsive">
  </div>



  <div class="text-center">
    <form class="form-inline" action="/" method="post">
      <div class="form-group">
        <label for="inputName1">Starting Location:</label>
        <input type="text" name="startingLoc" placeholder="Enter start location" class="form-control">
      </div>
      <div class="form-group">
        <label for="inputName1">Destination:</label>
        <input type="text" name="destination" placeholder="Enter destination" class="form-control">
      </div>
      <button type="submit" class="btn btn-primary">Submit</button>
    </form>
  </div>


  </body>
</html>
